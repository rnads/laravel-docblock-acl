{
    "name": "tjgazel/laravel-docblock-acl",
    "description": "ACL for laravel using docblock",
    "keywords": [
        "docblock",
        "acl",
        "laravel",
        "php",
        "tjgazel"
    ],
    "homepage": "https://github.com/tjgazel/laravel-docblock-acl",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Talles Gazel",
            "email": "tjgazel@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/rnads/laravel-docblock-acl"
        }
    ]
    "require-dev": {
        "illuminate/support": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/routing": "^8.0",
        "illuminate/contracts": "^8.0"
    },
    "autoload": {
        "files": [
            "helpers/helpers.php"
        ],
        "psr-4": {
            "TJGazel\\LaravelDocBlockAcl\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "TJGazel\\LaravelDocBlockAcl\\AclServiceProvider"
            ],
            "aliases": {
                "Acl": "TJGazel\\LaravelDocBlockAcl\\Facades\\Acl"
            }
        }
    }
}
